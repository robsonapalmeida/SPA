  <p id="notice"><%= notice %></p>

<form>

    <div class="form-group col-md-4">
      <%= label_tag nil, @user_x_service.class.human_attribute_name(:name) %>
      <%= text_field_tag nil, @user_x_service.name, class:"form-control", disabled: true %>
    </div>

    <div class="form-group col-md-4">
      <%= label_tag nil, @user_x_service.class.human_attribute_name(:email) %>
      <%= text_field_tag nil, @user_x_service.email, class:"form-control", disabled: true %>
    </div>

    <div class="form-group col-md-4">
      <%= label_tag nil, @user_x_service.class.human_attribute_name(:service) %>
      <%= text_field_tag nil,  @user_x_service.service.name, class:"form-control", disabled: true     %>
    </div>

    <div class="form-group col-md-4">
      <%= label_tag nil,  @user_x_service.class.human_attribute_name(:servicedate) %>
      <%= text_field_tag nil,  @user_x_service.servicedate.strftime("%d/%m/%Y"), class:"form-control", disabled: true    %>
    </div>

    <div class="form-group col-md-4">
      <%= label_tag nil,  @user_x_service.class.human_attribute_name(:servicedate) %>
      <%= text_field_tag nil,  @user_x_service.servicetime.strftime("%H:%M:%S"), class:"form-control", disabled: true    %>
    </div>

  <div class="form-group col-md-4">
      <%= label_tag nil, @user_x_service.class.human_attribute_name(:message) %>
      <%= text_field_tag nil, @user_x_service.message, class:"form-control", disabled: true    %>
    </div>



</form>    

<%= link_to 'Modificar', edit_user_x_service_path(@user_x_service),class:"btn btn-primary" %> |
<%= link_to 'Volver', user_x_services_path,class:"btn btn-primary" %>


